{
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" },
      { "src": "swagger-ui-dist/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/api-docs/(.*)", "dest": "/swagger-ui-dist/$1" }
    ]
  }
  